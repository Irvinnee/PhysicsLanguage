system układ1:
    particle cz1
    cz1->position = [0.0, 0.0, 0.0]
    cz1->velocity = [1.0, 0.0, 0.0]

$TIME = 0                % globalny zegar

law zwiększ_prędkość (particle p):
    if ($TIME < 5):
        p->velocity[0] = p->velocity[0] + 0.5

law hamulec (particle p):
    if ($TIME >= 8):
        p->velocity[0] = 0.0

<układ1 & zwiększ_prędkość>
<układ1 & hamulec>

run(układ1, 10)
print(cz1->position)
