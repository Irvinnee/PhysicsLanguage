$TIME = 100

law spring_force(particle p):
    float vx = p->velocity[0] - p->k * p->position[0]
    float x = p->position[0] + vx

    p->velocity = [vx, 0, 0]
    p->position = [x, 0, 0]


particle m1
particle m2

m1->position = [5, 0, 0]
m1->velocity = [0, 0, 0]
m1->k = 0.2

print(m1->position)

